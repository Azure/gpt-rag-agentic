# Application Insights connection string for telemetry and monitoring.
APPLICATIONINSIGHTS_CONNECTION_STRING=InstrumentationKey=<your-instrumentation-key>;IngestionEndpoint=https://<region>.in.applicationinsights.azure.com/;LiveEndpoint=https://<region>.livediagnostics.monitor.azure.com/;ApplicationId=<your-application-id>

# Orchestration strategy to be used (e.g., multimodal_rag)
AUTOGEN_ORCHESTRATION_STRATEGY=multimodal_rag

# Cosmos DB Configuration
AZURE_DB_ID=<your-cosmos-db-account-id>
AZURE_DB_NAME=<your-cosmos-db-name>

# Key Vault Configuration (used for retrieving secrets)
AZURE_KEY_VAULT_NAME=<your-key-vault-name>

# Azure OpenAI Settings
AZURE_OPENAI_API_KEY=<your-openai-api-key>
AZURE_OPENAI_API_VERSION=2024-10-21
AZURE_OPENAI_CHATGPT_DEPLOYMENT=chat
AZURE_OPENAI_CHATGPT_MODEL=gpt-4o
AZURE_OPENAI_EMBEDDING_DEPLOYMENT=text-embedding
AZURE_OPENAI_EMBEDDING_MODEL=text-embedding-3-large
AZURE_OPENAI_MAX_TOKENS=1000
AZURE_OPENAI_RESOURCE=azure-oai-common

# Azure Search Settings
AZURE_SEARCH_API_VERSION=2024-07-01
AZURE_SEARCH_APPROACH=hybrid
AZURE_SEARCH_INDEX=ragindex
AZURE_SEARCH_SEMANTIC_SEARCH_CONFIG=my-semantic-config
AZURE_SEARCH_SERVICE=ai-search-common
AZURE_SEARCH_TOP_K=3
AZURE_SEARCH_USE_SEMANTIC=false

# Azure Storage Settings (for documents)
AZURE_STORAGE_ACCOUNT=<your-storage-account>

